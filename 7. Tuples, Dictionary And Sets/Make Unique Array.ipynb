    "def duplicateCount(arr):\n",
    "    s = set()\n",
    "    count = 0\n",
    "    for i in arr:\n",
    "        if i in s:\n",
    "            count += 1\n",
    "        s.add(i)\n",
    "    return count\n",
    "    \n",
    "    \n",
    "n = int(input())\n",
    "arr = [int(i) for i in input().split()]\n",
    "ans = duplicateCount(arr)\n",
    "print(ans)"
   ]
